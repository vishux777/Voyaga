{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Voyaga Django Server",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/backend/manage.py",
      "args": ["runserver", "8000"],
      "django": true,
      "cwd": "${workspaceFolder}/backend",
      "env": {
        "DJANGO_SETTINGS_MODULE": "voyaga.settings"
      }
    }
  ]
}
