{
  "python.defaultInterpreterPath": "${workspaceFolder}/venv/Scripts/python.exe",
  "python.terminal.activateEnvironment": true,
  "files.exclude": {
    "**/__pycache__": true,
    "**/*.pyc": true,
    "**/migrations": false
  },
  "[python]": {
    "editor.formatOnSave": true
  },
  "python.analysis.extraPaths": ["${workspaceFolder}/backend"]
}
